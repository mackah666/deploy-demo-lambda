{
    "name": "awaze-location-service",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "clean": "rm -R -f dist",
        "check": "npx prettier --check .",
        "format": "npx prettier --write .",
        "build": "esbuild --platform=node --target=node14 --outdir=dist ./src/*.js && mkdir dist/node_modules && cp -R ./node_modules/** ./dist/node_modules",
        "lint": "eslint src --fix --ext .js",
        "package": "zip -r -j location-service.zip dist/**"
    },
    "engines": {
        "node": "14.17.5"
    },
    "dependencies": {
        "axios": ">=0.21.2",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "copyfiles": "^2.4.1",
        "esbuild": "^0.13.3",
        "eslint": "^7.32.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "^2.24.2",
        "npm-pack-zip": "^1.2.9",
        "prettier": "2.4.1",
        "webpack": "^5.55.1",
        "webpack-cli": "^4.8.0"
    }
}
