{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
        "no-use-before-define": ["error", {"functions": false, "classes": false}]
    }
}
